#VolumeUp
"amixer sset Master 1+"
    XF86AudioRaiseVolume

#VolumeDown
"amixer sset Master 1-"
    XF86AudioLowerVolume

#Mute
"amixer sset Master toggle"
    XF86AudioMute

#PlayPause
"mpc toggle"
    XF86AudioPlay

#Stop
"mpc stop"
    XF86AudioStop

#Previous
"mpc prev"
    XF86AudioPrev

#Next
"mpc next"
    XF86AudioNext

#Lock
"lock"
   XF86ScreenSaver

#Toggle-Vga
"xrandr-config"
   XF86Display

#Toggle-Touchpad
"toggle-touchpad"
   XF86TouchpadOn
