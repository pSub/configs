#VolumeUp
"amixer sset Master 1+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

#VolumeDown
"amixer sset Master 1-"
    m:0x0 + c:122
    XF86AudioLowerVolume

#Mute
"amixer sset Master toggle"
    m:0x0 + c:121
    XF86AudioMute

#PlayPause
"mocp -G"
    m:0x0 + c:172
    XF86AudioPlay

#Stop
"mocp -s"
    m:0x0 + c:174
    XF86AudioStop

#Previous
"mocp -r"
    m:0x0 + c:173
    XF86AudioPrev

#Next
"mocp -f"
    m:0x0 + c:171
    XF86AudioNext

#Lock
"slock"
   m:0x0 + c:160
   XF86ScreenSaver

#Toggle-Vga
"xrandr-config"
   m:0x0 + c:214
   XF86Display

#Toggle-Touchpad
"toggle-touchpad"
   m:0x0 + c:200
   XF86TouchpadOn
