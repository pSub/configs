#VolumeUp
"amixer -q sset Master 2+"
    XF86AudioRaiseVolume

#VolumeDown
"amixer -q sset Master 2-"
    XF86AudioLowerVolume

#Mute
"amixer -q sset Master toggle"
    XF86AudioMute

#PlayPause
"mpc toggle"
    XF86AudioPlay

#Stop
"mpc stop"
    XF86AudioStop

#Previous
"mpc prev"
    XF86AudioPrev

#Next
"mpc next"
    XF86AudioNext

#Lock
"lock"
   XF86ScreenSaver

#Toggle-Vga
"xrandr-config"
   XF86Display

#Toggle-Touchpad
"toggle-touchpad"
   XF86TouchpadOn
