#VolumeUp
"pamixer --increase 2"
    XF86AudioRaiseVolume

#VolumeDown
"pamixer --decrease 2"
    XF86AudioLowerVolume

#Mute
"pamixer --toggle-mute"
    XF86AudioMute

#PlayPause
"mpc toggle"
    XF86AudioPlay

#Stop
"mpc stop"
    XF86AudioStop

#Previous
"mpc prev"
    XF86AudioPrev

#Next
"mpc next"
    XF86AudioNext

#Lock
"lock"
   XF86ScreenSaver

#Toggle-Vga
"xrandr-config"
   XF86Display

#Toggle-Touchpad
"toggle-touchpad"
   XF86TouchpadOn
