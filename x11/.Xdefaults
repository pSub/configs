Xcursor.theme                : Neutral
Xft.dpi                      : 96
Xft.antialias                : true
Xft.hinting                  : full

! --- emacs --- !
emacs.pointerColor           : #000000

! --- rxvt-unicode --- !
urxvt.font                   : xft:inconsolata:pixelsize=13,xft:dejavu sans mono:pixelsize=13,xft:gentium

! Slightly modified Solarized color-scheme
#define S_base03        #002b36
#define S_base02        #073642
#define S_base01        #586e75
#define S_base00        #657b83
#define S_base0         #839496
#define S_base1         #93a1a1
#define S_base2         #eee8d5
#define S_base3         #fdf6e3
#define S_yellow        #b58900
#define S_orange        #cb4b16
#define S_red           #dc322f
#define S_magenta       #d33682
#define S_violet        #6c71c4
#define S_blue          #268bd2
#define S_cyan          #2aa198
#define S_green         #859900

*background:            #f9f7f3
*foreground:            S_base00
*fading:                40
*fadeColor:             S_base3
*cursorColor:           S_base01
*pointerColorBackground:S_base1
*pointerColorForeground:S_base01
*color0:                S_base02
*color8:                S_base03
*color1:                S_red
*color9:                S_orange
*color2:                S_green
*color10:               S_base01
*color3:                S_yellow
*color11:               S_base00
*color4:                S_blue
*color12:               S_base0
*color5:                S_magenta
*color13:               S_violet
*color6:                S_cyan
*color14:               S_base1
*color7:                S_base2
*color15:               S_base3

urxvt.urgentOnBell           : true

urxvt.internalBorder         : 3
urxvt.saveLines              : 65535
urxvt.visualBell             : false

! No scroll bar.
urxvt.scrollBar              : plain

urxvt.scrollTtyOutput        : false
urxvt.scrollTtyKeypress      : true
urxvt.scrollWithBuffer       : true
urxvt.jumpScroll             : true
urxvt.secondaryScroll        : true
urxvt.utmpInhibit            : true

! Was urlLauncher in urxvt versions before 9.16.
urxvt.url-launcher           : dwb

! Extension that matches URLs.
urxvt.perl-ext-common        : default,matcher

urxvt.matcher.pattern.1      : \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
urxvt.matcher.button         : 1

urxvt.depth                  : 24
urxvt.transparent            : false

! If the window has no focus its faded out about 5%
urxvt.fading                 : 5%
urxvt.fadeColor              : gray

urxvt.metaSendsEscape        : true
urxvt.pastableTabs           : true

! Pressing Print dumps the screen content to a file
urxvt.print-pipe             : cat > $HOME/urxvt-screen-dump

! Charactes which bound the selection on double click.
urxvt.cutchars               : BACKSLASH `&()*,;<=>?@[]^{|}-"'"
