# jumanji configuration
searchengine g https://www.duckduckgo.com/?q=%s

set tabbar_bgcolor #222222
set tabbar_fgcolor #ffffff
set tabbar_focus_bgcolor #2e3436
set tabbar_focus_fgcolor #f5a400

set download_dir ~/sandbox/
set download_command filechooser '%s' ~/.config.jumanji/cookies
set spawn_editor ec
